{
    "camelcase": true,
    "curly": true,
    "freeze": true,    
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "maxlen": 120,
    "bitwise": true,
    "eqeqeq": true,
    "strict": true,
    "trailing": true,
    "maxparams": 5,
    "maxdepth": 5,
    "maxstatements": 40,
    "maxcomplexity": 5,
    "noarg": true,
    "sub": true,
    "eqnull": true,
    "globals": {
        "angular": true,
        "window": true,
        "document": true,
        "console": true,
        "describe": true,
        "xdescribe": true,
        "ddescribe": true,
        "it": true,
        "xit": true,
        "iit": true,
        "beforeEach": true,
        "module": true,
        "inject": true,
        "expect": true
    }
}